{
  "name": "waitingListStudent",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "scope": {
    "include": [
      {
        "relation": "student"
      }
    ]
  },
  "properties": {
    "waitingListId": {
      "type": "number",
      "required": true
    },
    "studentId": {
      "type": "number",
      "required": true
    },
    "branchId": {
      "type": "number",
      "required": true
    },
    "note": {
      "type": "string",
      "required": false
    },
    "createdAt": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "student": {
      "type": "belongsTo",
      "model": "student",
      "foreignKey": "studentId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "super-admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
