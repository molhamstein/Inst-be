{
  "name": "unit",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "scope": {
    "include": [
      {
        "relation": "videos"
      }
    ]
  },
  "properties": {
    "courseId": {
      "type": "number",
      "required": true
    },
    "nameEn": {
      "type": "string",
      "required": true
    },
    "nameAr": {
      "type": "string",
      "required": true
    },
    "videosCount": {
      "type": "number",
      "required": true
    },
    "createdAt": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "videos": {
      "type": "hasMany",
      "model": "video",
      "foreignKey": "unitId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
