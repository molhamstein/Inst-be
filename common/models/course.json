      "type": "number",
      "required": true
    },
    "countStudent": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "countStudentInQueue": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "sessionAvgDuration": {
      "type": "number",
      "required": true
    },
    "waitingListId": {
      "type": "number",
      "required": false
    },
    "status": {
      "type": "string",
      "required": true,
      "default": "active"
    },
    "createdAt": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "institute": {
      "type": "belongsTo",
      "model": "institute",
      "foreignKey": "instituteId",
      "options": {
        "nestRemoting": true
      }
    },
    "branch": {
      "type": "belongsTo",
      "model": "branch",
      "foreignKey": "branchId",
      "options": {
        "nestRemoting": true
      }
    },
    "subcategory": {
      "type": "belongsTo",
      "model": "subCategory",
      "foreignKey": "subcategoryId",
      "options": {
        "nestRemoting": true
      }
    },
    "media": {
      "type": "hasMany",
      "model": "media",
      "foreignKey": "courseId",
      "options": {
        "nestRemoting": true
      },
      "through": "courseImages",
      "keyThrough": "imageId"
    },
    "waitingList": {
      "type": "belongsTo",
      "model": "waitingList",
      "foreignKey": "waitingListId",
      "options": {
        "nestRemoting": true
      }
    }
  },
