{
  "name": "studentPayment",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "scope": {
    "include": [{
      "relation": "course"
    }]
  },

  "properties": {
    "studentId": {
      "type": "number",
      "required": true
    },
    "value": {
      "type": "number",
      "required": true
    },
    "courseId": {
      "type": "number",
      "required": false
    },
    "date": {
      "type": "date",
      "required": true
    },
    "note": {
      "type": "string",
      "required": false
    },
    "createdAt": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "course": {
      "type": "belongsTo",
      "model": "course",
      "foreignKey": "courseId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
