{
  "name": "studentSession",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "scope": {
    "include": [{
      "relation": "student"
    }]
  },
  "properties": {
    "sessionId": {
      "type": "number",
      "required": true
    },
    "studentId": {
      "type": "number",
      "required": true
    },
    "balance": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "isAttend": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "dateAttend": {
      "type": "date",
      "required": false
    },
    "cost": {
      "type": "number",
      "required": false
    },
    "createdAt": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "student": {
      "type": "belongsTo",
      "model": "student",
      "foreignKey": "studentId",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [{
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "super-admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
