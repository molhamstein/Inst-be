      "type": "number",
      "required": true
    },
    "studentId": {
      "type": "number",
      "required": true
    },
    "frozenBalance": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "balance": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "cost": {
      "type": "number",
      "required": false
    },
    "createdAt": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
